{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "types": ["react-native"],
    "paths": {
      "@/*": ["src/*"],
      "@assets/*": ["src/assets/*"],
      "@screens/*": ["src/screens/*"],
      "@constants/*": ["src/constants/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
